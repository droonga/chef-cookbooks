<% engine = node.droonga.engine %>
{
  "version": 2,
  "effectiveDate": "2014-01-24T00:00:00Z",
  "datasets": {
    "Droonga": {
      "nWorkers": 2,
      "plugins": ["groonga", "crud", "search"],
      "replicas": [
        {
          "dimension": "_key",
          "slicer": "hash",
          "slices": [
            {
              "label": "slice000",
              "weight": 50,
              "volume": {
                "address": "127.0.0.1:<%= engine.port %>/<%= engine.tag %>.000"
              }
            },
            {
              "label": "slice001",
              "weight": 50,
              "volume": {
                "address": "127.0.0.1:<%= engine.port %>/<%= engine.tag %>.001"
              }
            }
          ]
        },
        {
          "dimension": "_key",
          "slicer": "hash",
          "slices": [
            {
              "label": "slice010",
              "weight": 50,
              "volume": {
                "address": "127.0.0.1:<%= engine.port %>/<%= engine.tag %>.010"
              }
            },
            {
              "label": "slice011",
              "weight": 50,
              "volume": {
                "address": "127.0.0.1:<%= engine.port %>/<%= engine.tag %>.011"
              }
            }
          ]
        }
      ]
    }
  }
}
